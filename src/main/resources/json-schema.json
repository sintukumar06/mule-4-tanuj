{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "Address": {
      "type": "object",
      "properties": {
        "city": {
          "type": "string"
        },
        "zip": {
          "type": "integer"
        }
      },
      "required": [
        "city",
        "zip"
      ]
    }
  },
  "required": [
    "id",
    "name"
  ]
}